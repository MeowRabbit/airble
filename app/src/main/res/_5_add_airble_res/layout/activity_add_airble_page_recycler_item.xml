<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="._5_add_airble.AddAirbleActivity"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="24dp"
    android:layout_marginEnd="24dp"
    android:layout_marginTop="13dp"
    android:layout_marginBottom="13dp"
    android:orientation="horizontal"
    android:gravity="center_vertical"
    android:id="@+id/add_airble_page_2_recycler_item_LinearLayout">
    
    <View
        android:layout_width="31dp"
        android:layout_height="22.5dp"
        android:background="@drawable/add_airble_page_2_wifi_icon"/>

    <TextView
        android:id="@+id/add_airble_page_2_recycler_item_TextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="공유기 이름"
        android:textColor="#508EF9"
        android:fontFamily="@font/gothic_a1_500_medium"
        android:textSize="17dp"
        android:includeFontPadding="false"
        android:singleLine="true"
        android:ellipsize="end"
        android:layout_marginTop="2dp"
        android:layout_marginStart="16dp" />

</LinearLayout>