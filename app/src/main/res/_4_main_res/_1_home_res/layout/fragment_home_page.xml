<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="._4_main.MainActivity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#FFF">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="171dp"
        android:layout_marginTop="10dp">

        <stm.airble._0_public.NestedScrollableHost
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.viewpager2.widget.ViewPager2
                android:id="@+id/main_home_ViewPager2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>

        </stm.airble._0_public.NestedScrollableHost>


        <ImageButton
            android:id="@+id/main_home_ViewPager2_left_arrow_ImageButton"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:src="@drawable/back"
            android:scaleType="fitCenter"
            android:background="#00000000"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:paddingStart="5dp"
            android:paddingEnd="15dp"
            tools:ignore="RtlSymmetry"
            app:tint="#E4EFFF" />

        <ImageButton
            android:id="@+id/main_home_ViewPager2_right_arrow_ImageButton"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:src="@drawable/back"
            android:scaleType="fitCenter"
            android:background="#00000000"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:paddingStart="5dp"
            android:paddingEnd="15dp"
            tools:ignore="RtlSymmetry"
            app:tint="#E4EFFF"
            android:rotation="180" />

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0px"
        android:layout_weight="1"
        android:orientation="vertical"
        android:layout_margin="5dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="5"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/home_matter_block_shadow"
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:orientation="vertical"/>

                <RelativeLayout
                    android:id="@+id/main_home_matter_vocs_block_RelativeLayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/home_matter_block"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/main_home_matter_vocs_TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="6dp"
                        android:fontFamily="@font/ubuntu_bold"
                        android:text="VOCs"
                        android:textColor="@color/app_main_font_color"
                        android:textSize="30dp" />

                    <TextView
                        android:id="@+id/main_home_matter_vocs_trans_TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/main_home_matter_vocs_TextView"
                        android:layout_alignStart="@id/main_home_matter_vocs_TextView"
                        android:layout_marginTop="-7dp"
                        android:fontFamily="@font/gothic_a1_500_medium"
                        android:text="휘발성유기화합물"
                        android:textColor="@color/app_main_font_color"
                        android:textSize="12dp" />

                    <TextView
                        android:id="@+id/main_home_matter_vocs_value_TextView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@id/main_home_matter_vocs_unit_TextView"
                        android:layout_marginEnd="3dp"
                        android:layout_marginBottom="-7dp"
                        android:layout_toStartOf="@id/main_home_matter_vocs_unit_TextView"
                        android:fontFamily="@font/gothic_a1_700_bold"
                        android:gravity="right"
                        android:text="1.000"
                        android:textColor="#444"
                        android:textSize="25dp" />

                    <TextView
                        android:id="@+id/main_home_matter_vocs_unit_TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_above="@id/main_home_matter_vocs_status_TextView"
                        android:layout_alignEnd="@id/main_home_matter_vocs_status_TextView"
                        android:layout_marginBottom="3dp"
                        android:fontFamily="@font/ubuntu_regular"
                        android:paddingBottom="1dp"
                        android:text="㎎/㎥"
                        android:textColor="#444"
                        android:textSize="15dp" />

                    <TextView
                        android:id="@+id/main_home_matter_vocs_status_TextView"
                        android:layout_width="55dp"
                        android:layout_height="15dp"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:layout_gravity="end"
                        android:layout_marginEnd="9dp"
                        android:layout_marginBottom="5dp"
                        android:background="@drawable/home_matter_status_good_block"
                        android:fontFamily="@font/gothic_a1_500_medium"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:paddingTop="1dp"
                        android:text="좋음"
                        android:textColor="#FFF"
                        android:textSize="12dp" />

                </RelativeLayout>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/home_matter_block_shadow"
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:orientation="vertical"/>

                <RelativeLayout
                    android:id="@+id/main_home_matter_co2_block_RelativeLayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/home_matter_block"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/main_home_matter_co2_TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="6dp"
                        android:fontFamily="@font/ubuntu_bold"
                        android:text="CO₂"
                        android:textColor="@color/app_main_font_color"
                        android:textSize="30dp" />

                    <TextView
                        android:id="@+id/main_home_matter_co2_trans_TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/main_home_matter_co2_TextView"
                        android:layout_alignStart="@id/main_home_matter_co2_TextView"
                        android:layout_marginTop="-7dp"
                        android:fontFamily="@font/gothic_a1_500_medium"
                        android:text="이산화탄소"
                        android:textColor="@color/app_main_font_color"
                        android:textSize="12dp" />

                    <TextView
                        android:id="@+id/main_home_matter_co2_value_TextView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@id/main_home_matter_co2_unit_TextView"
                        android:layout_marginEnd="3dp"
                        android:layout_marginBottom="-7dp"
                        android:layout_toStartOf="@id/main_home_matter_co2_unit_TextView"
                        android:fontFamily="@font/gothic_a1_700_bold"
                        android:gravity="right"
                        android:text="1.000"
                        android:textColor="#444"
                        android:textSize="25dp" />

                    <TextView
                        android:id="@+id/main_home_matter_co2_unit_TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_above="@id/main_home_matter_co2_status_TextView"
                        android:layout_alignEnd="@id/main_home_matter_co2_status_TextView"
                        android:layout_marginBottom="3dp"
                        android:fontFamily="@font/ubuntu_regular"
                        android:paddingBottom="1dp"
                        android:text="ppm"
                        android:textColor="#444"
                        android:textSize="15dp" />

                    <TextView
                        android:id="@+id/main_home_matter_co2_status_TextView"
                        android:layout_width="55dp"
                        android:layout_height="15dp"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:layout_gravity="end"
                        android:layout_marginEnd="9dp"
                        android:layout_marginBottom="5dp"
                        android:background="@drawable/home_matter_status_good_block"
                        android:fontFamily="@font/gothic_a1_500_medium"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:paddingTop="1dp"
                        android:text="좋음"
                        android:textColor="#FFF"
                        android:textSize="12dp" />

                </RelativeLayout>

            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="5"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/home_matter_block_shadow"
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:orientation="vertical"/>

                <RelativeLayout
                    android:id="@+id/main_home_matter_co_block_RelativeLayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/home_matter_block"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/main_home_matter_co_TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="6dp"
                        android:fontFamily="@font/ubuntu_bold"
                        android:text="CO"
                        android:textColor="@color/app_main_font_color"
                        android:textSize="30dp" />

                    <TextView
                        android:id="@+id/main_home_matter_co_trans_TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/main_home_matter_co_TextView"
                        android:layout_alignStart="@id/main_home_matter_co_TextView"
                        android:layout_marginTop="-7dp"
                        android:fontFamily="@font/gothic_a1_500_medium"
                        android:text="일산화탄소"
                        android:textColor="@color/app_main_font_color"
                        android:textSize="12dp" />

                    <TextView
                        android:id="@+id/main_home_matter_co_value_TextView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@id/main_home_matter_co_unit_TextView"
                        android:layout_marginEnd="3dp"
                        android:layout_marginBottom="-7dp"
                        android:layout_toStartOf="@id/main_home_matter_co_unit_TextView"
                        android:fontFamily="@font/gothic_a1_700_bold"
                        android:gravity="right"
                        android:text="1.000"
                        android:textColor="#444"
                        android:textSize="25dp" />

                    <TextView
                        android:id="@+id/main_home_matter_co_unit_TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_above="@id/main_home_matter_co_status_TextView"
                        android:layout_alignEnd="@id/main_home_matter_co_status_TextView"
                        android:layout_marginBottom="3dp"
                        android:fontFamily="@font/ubuntu_regular"
                        android:paddingBottom="1dp"
                        android:text="ppm"
                        android:textColor="#444"
                        android:textSize="15dp" />

                    <TextView
                        android:id="@+id/main_home_matter_co_status_TextView"
                        android:layout_width="55dp"
                        android:layout_height="15dp"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:layout_gravity="end"
                        android:layout_marginEnd="9dp"
                        android:layout_marginBottom="5dp"
                        android:background="@drawable/home_matter_status_good_block"
                        android:fontFamily="@font/gothic_a1_500_medium"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:paddingTop="1dp"
                        android:text="좋음"
                        android:textColor="#FFF"
                        android:textSize="12dp" />

                </RelativeLayout>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/home_matter_block_shadow"
                    android:layout_marginTop="2dp"
                    android:layout_marginStart="2dp"
                    android:orientation="vertical"/>

                <RelativeLayout
                    android:id="@+id/main_home_matter_pm_block_RelativeLayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/home_matter_block"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/main_home_matter_pm_TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="6dp"
                        android:fontFamily="@font/ubuntu_bold"
                        android:text="PM2.5"
                        android:textColor="@color/app_main_font_color"
                        android:textSize="30dp" />

                    <TextView
                        android:id="@+id/main_home_matter_pm_trans_TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/main_home_matter_pm_TextView"
                        android:layout_alignStart="@id/main_home_matter_pm_TextView"
                        android:layout_marginTop="-7dp"
                        android:fontFamily="@font/gothic_a1_500_medium"
                        android:text="초미세먼지"
                        android:textColor="@color/app_main_font_color"
                        android:textSize="12dp" />

                    <TextView
                        android:id="@+id/main_home_matter_pm_value_TextView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@id/main_home_matter_pm_unit_TextView"
                        android:layout_marginEnd="3dp"
                        android:layout_marginBottom="-7dp"
                        android:layout_toStartOf="@id/main_home_matter_pm_unit_TextView"
                        android:fontFamily="@font/gothic_a1_700_bold"
                        android:gravity="right"
                        android:text="1.000"
                        android:textColor="#444"
                        android:textSize="25dp" />

                    <TextView
                        android:id="@+id/main_home_matter_pm_unit_TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_above="@id/main_home_matter_pm_status_TextView"
                        android:layout_alignEnd="@id/main_home_matter_pm_status_TextView"
                        android:layout_marginBottom="3dp"
                        android:fontFamily="@font/ubuntu_regular"
                        android:paddingBottom="1dp"
                        android:text="㎍/㎥"
                        android:textColor="#444"
                        android:textSize="15dp" />

                    <TextView
                        android:id="@+id/main_home_matter_pm_status_TextView"
                        android:layout_width="55dp"
                        android:layout_height="15dp"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:layout_gravity="end"
                        android:layout_marginEnd="9dp"
                        android:layout_marginBottom="5dp"
                        android:background="@drawable/home_matter_status_good_block"
                        android:fontFamily="@font/gothic_a1_500_medium"
                        android:gravity="center"
                        android:includeFontPadding="false"
                        android:paddingTop="1dp"
                        android:text="좋음"
                        android:textColor="#FFF"
                        android:textSize="12dp" />

                </RelativeLayout>

            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1.8"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_margin="5dp"
                android:orientation="vertical">


                <TextView
                    android:id="@+id/main_home_matter_temp_unit_TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="℃"
                    android:textSize="13dp"
                    android:fontFamily="@font/dm_sans_medium"
                    android:textColor="@color/home_matter_temp_color"
                    android:alpha="0.5"
                    android:layout_alignParentEnd="true"
                    android:layout_alignBottom="@id/main_home_matter_temp_value_TextView"
                    android:layout_marginEnd="3dp"
                    android:layout_marginBottom="3.5dp"
                    android:gravity="bottom"/>

                <TextView
                    android:id="@+id/main_home_matter_temp_value_TextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="end"
                    android:text="31"
                    android:textSize="25dp"
                    android:fontFamily="@font/dm_sans_medium"
                    android:textColor="@color/home_matter_temp_color"
                    android:layout_toStartOf="@id/main_home_matter_temp_unit_TextView"
                    android:layout_centerVertical="true"
                    android:paddingTop="10dp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:layout_marginStart="8dp"
                    android:text="온도"
                    android:textSize="17dp"
                    android:fontFamily="@font/gothic_a1_700_bold"
                    android:textColor="@color/home_matter_temp_color"
                    android:layout_toStartOf="@id/main_home_matter_temp_unit_TextView"
                    android:layout_alignBottom="@id/main_home_matter_temp_value_TextView"
                    android:layout_marginBottom="3.5dp"/>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_margin="5dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/main_home_matter_humi_unit_TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="%"
                    android:textSize="13dp"
                    android:fontFamily="@font/dm_sans_medium"
                    android:textColor="@color/home_matter_humi_color"
                    android:alpha="0.5"
                    android:layout_alignParentEnd="true"
                    android:layout_alignBottom="@id/main_home_matter_humi_value_TextView"
                    android:layout_marginEnd="3dp"
                    android:layout_marginBottom="3.5dp"/>

                <TextView
                    android:id="@+id/main_home_matter_humi_value_TextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="end"
                    android:text="19"
                    android:textSize="25dp"
                    android:fontFamily="@font/dm_sans_medium"
                    android:textColor="@color/home_matter_humi_color"
                    android:layout_toStartOf="@id/main_home_matter_humi_unit_TextView"
                    android:layout_centerVertical="true"
                    android:paddingTop="10dp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:layout_marginStart="8dp"
                    android:text="습도"
                    android:textSize="17dp"
                    android:fontFamily="@font/gothic_a1_700_bold"
                    android:textColor="@color/home_matter_humi_color"
                    android:layout_toStartOf="@id/main_home_matter_humi_unit_TextView"
                    android:layout_alignBottom="@id/main_home_matter_humi_value_TextView"
                    android:layout_marginBottom="3.5dp"/>

            </RelativeLayout>

        </LinearLayout>


    </LinearLayout>

</LinearLayout>
