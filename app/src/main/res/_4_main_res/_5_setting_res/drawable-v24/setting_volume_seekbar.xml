<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--시크바 배경-->
    <item android:id="@android:id/background"
        android:top="5dp"
        android:bottom="5dp">
        <shape android:shape="rectangle">    <!--선 : 길이6dp -->
            <corners android:radius="10dp"/>
            <solid android:color="#4D508EF9"/>
        </shape>
    </item>


    <!-- 시크바 프로그래스 -->
    <item android:id="@android:id/progress"
        android:top="5dp"
        android:bottom="5dp" >
        <clip>
            <shape android:shape="rectangle">    <!--선 : 길이6dp -->
                <corners android:radius="10dp"/>
                <solid android:color="#B2508EF9"/>
            </shape>
        </clip>
    </item>

</layer-list>
