<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="._4_main.MainActivity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="10dp"
    android:id="@+id/settings_airble_DrawerLayout">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#FFF">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageButton
                android:id="@+id/back_Button"
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="14dp"
                android:padding="10dp"
                android:background="#00000000"
                android:scaleType="fitCenter"
                android:src="@drawable/setting_back"/>

            <TextView
                android:id="@+id/settings_airble_title_TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginTop="27dp"
                android:layout_marginBottom="27dp"
                android:fontFamily="@font/gothic_a1_700_bold"
                android:text="기기 설정"
                android:textColor="#508EF9"
                android:textSize="15dp" />

        </RelativeLayout>

        <LinearLayout
            android:id="@+id/settings_airble_nickname_LinearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="5dp"
            android:layout_marginStart="15.5dp"
            android:layout_marginEnd="15.5dp"
            android:background="@drawable/setting_block_shadow"
            android:padding="4dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@drawable/setting_block">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="이름"
                    android:textSize="17dp"
                    android:textColor="#508EF9"
                    android:fontFamily="@font/gothic_a1_700_bold"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="18dp"
                    android:layout_marginBottom="18dp"/>

                <TextView
                    android:id="@+id/settings_airble_nickname_TextView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="기기 명"
                    android:textSize="17dp"
                    android:textColor="#508EF9"
                    android:fontFamily="@font/gothic_a1_700_bold"
                    android:layout_marginEnd="20dp"
                    android:layout_marginTop="18dp"
                    android:layout_marginBottom="18dp"
                    android:gravity="end"
                    android:alpha="0.7"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/settings_airble_information_LinearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="7dp"
            android:layout_marginStart="15.5dp"
            android:layout_marginEnd="15.5dp"
            android:background="@drawable/setting_block_shadow"
            android:padding="4dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@drawable/setting_block">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="기기 정보"
                    android:textSize="17dp"
                    android:textColor="#508EF9"
                    android:fontFamily="@font/gothic_a1_700_bold"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="18dp"
                    android:layout_marginBottom="18dp"/>

                <ImageView
                    android:layout_width="17dp"
                    android:layout_height="17dp"
                    android:src="@drawable/setting_back"
                    android:scaleType="fitCenter"
                    android:layout_marginEnd="18.5dp"
                    android:layout_gravity="center"
                    android:rotation="180"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/settings_airble_volume_LinearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="7dp"
            android:layout_marginStart="15.5dp"
            android:layout_marginEnd="15.5dp"
            android:background="@drawable/setting_block_shadow"
            android:padding="4dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@drawable/setting_block">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="볼륨"
                    android:textSize="17dp"
                    android:textColor="#508EF9"
                    android:fontFamily="@font/gothic_a1_700_bold"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="18dp"
                    android:layout_marginBottom="18dp"/>

                <TextView
                    android:id="@+id/settings_airble_volume_TextView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="100%"
                    android:textSize="17dp"
                    android:textColor="#508EF9"
                    android:fontFamily="@font/gothic_a1_700_bold"
                    android:layout_marginEnd="20dp"
                    android:layout_marginTop="18dp"
                    android:layout_marginBottom="18dp"
                    android:gravity="end"
                    android:alpha="0.7"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/settings_airble_place_LinearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="7dp"
            android:layout_marginStart="15.5dp"
            android:layout_marginEnd="15.5dp"
            android:background="@drawable/setting_block_shadow"
            android:padding="4dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@drawable/setting_block">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="위치"
                    android:textSize="17dp"
                    android:textColor="#508EF9"
                    android:fontFamily="@font/gothic_a1_700_bold"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="18dp"
                    android:layout_marginBottom="18dp"/>

                <TextView
                    android:id="@+id/settings_airble_place_TextView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="서울특별시"
                    android:textSize="15dp"
                    android:textColor="#508EF9"
                    android:fontFamily="@font/gothic_a1_700_bold"
                    android:layout_marginEnd="20dp"
                    android:layout_marginTop="18dp"
                    android:layout_marginBottom="18dp"
                    android:gravity="end"
                    android:alpha="0.7"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/settings_airble_shared_LinearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="7dp"
            android:layout_marginStart="15.5dp"
            android:layout_marginEnd="15.5dp"
            android:background="@drawable/setting_block_shadow"
            android:padding="4dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@drawable/setting_block">

                <TextView
                    android:id="@+id/settings_airble_shared_TextView_1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="기기 공유"
                    android:textSize="17dp"
                    android:textColor="#508EF9"
                    android:fontFamily="@font/gothic_a1_700_bold"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="18dp"
                    android:layout_marginBottom="18dp"/>

                <TextView
                    android:id="@+id/settings_airble_shared_TextView_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text=""
                    android:textSize="17dp"
                    android:textColor="#508EF9"
                    android:fontFamily="@font/gothic_a1_700_bold"
                    android:layout_marginEnd="20dp"
                    android:layout_marginTop="18dp"
                    android:layout_marginBottom="18dp"
                    android:gravity="end"
                    android:alpha="0.7"
                    android:visibility="gone"/>

                <ImageView
                    android:layout_width="17dp"
                    android:layout_height="17dp"
                    android:src="@drawable/setting_back"
                    android:scaleType="fitCenter"
                    android:layout_marginEnd="18.5dp"
                    android:layout_gravity="center"
                    android:rotation="180"/>

            </LinearLayout>

        </LinearLayout>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_weight="1"/>

        <Button
            android:id="@+id/settings_reset_Button"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:text="데이터초기화"
            android:textSize="17dp"
            android:textColor="#508EF9"
            android:fontFamily="@font/gothic_a1_700_bold"
            style="?android:attr/borderlessButtonStyle"
            android:layout_marginStart="15.5dp"
            android:layout_marginBottom="10dp"
            android:includeFontPadding="false"/>

        <Button
            android:id="@+id/settings_remove_Button"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:text="기기삭제"
            android:textSize="17dp"
            android:textColor="#508EF9"
            android:fontFamily="@font/gothic_a1_700_bold"
            style="?android:attr/borderlessButtonStyle"
            android:layout_marginStart="15.5dp"
            android:layout_marginBottom="10dp"
            android:includeFontPadding="false"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/settings_airble_drawer_LinearLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:outlineProvider="none"
        android:background="#FFF"
        android:layout_gravity="end">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageButton
                android:id="@+id/settings_airble_drawer_back_Button"
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="14dp"
                android:padding="10dp"
                android:background="#00000000"
                android:scaleType="fitCenter"
                android:src="@drawable/setting_back"/>

            <TextView
                android:id="@+id/settings_airble_drawer_title_TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="27dp"
                android:layout_marginBottom="17dp"
                android:fontFamily="@font/gothic_a1_700_bold"
                android:text="기기 정보"
                android:textColor="#508EF9"
                android:textSize="15dp" />

        </RelativeLayout>

        <androidx.viewpager2.widget.ViewPager2
            android:id="@+id/settings_airble_drawer_ViewPager2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

    </LinearLayout>

</androidx.drawerlayout.widget.DrawerLayout>